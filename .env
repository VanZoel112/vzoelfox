API_ID=29919905
API_HASH=717957f0e3ae20a7db004d08b66bfd30

# ============= USERBOT SETTINGS =============
SESSION_NAME=userbot_session
LOG_LEVEL=INFO

# ============= SECURITY SETTINGS =============
# User ID yang diizinkan menggunakan userbot (kosongkan untuk auto-detect)
OWNER_ID=

# Password untuk beberapa command sensitive (opsional)
ADMIN_PASSWORD=

# ============= FEATURES TOGGLE =============
# Enable/disable features (true/false)
ENABLE_LOGGING=true
ENABLE_AUTO_RESTART=true
ENABLE_SPAM_PROTECTION=true
MAX_SPAM_COUNT=10

# ============= NOTIFICATION SETTINGS =============
# Chat ID untuk notifikasi (default: 'me' untuk Saved Messages)
NOTIFICATION_CHAT=me

# ============= DATABASE SETTINGS =============
# Untuk fitur advanced (opsional)
DATABASE_URL=sqlite:///userbot.db

# ============= HEROKU/CLOUD SETTINGS =============
# Untuk deployment di cloud (opsional)
PORT=8080
WEBHOOK_URL=

# ============= CUSTOM SETTINGS =============
# Timezone
TIMEZONE=Asia/Jakarta

# Custom prefix untuk commands (default: .)
COMMAND_PREFIX=.

# ============= DEVELOPMENT =============
DEBUG_MODE=false
