client
dev tun
proto udp
remote 13.236.95.51 1194
resolv-retry infinite
nobind
persist-key
persist-tun

# Security Settings
cipher AES-256-GCM
auth SHA256
key-direction 1

# Logging
verb 3
mute 20

# Keep connection alive
keepalive 10 120

# Pull routes from server
pull

# Certificate files - REAL CERTIFICATES!
ca ca.crt
cert client.crt  
key client.key

